{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "E-commerce using Nodejs, Typescript, TypeORM ORM, SQL, Express ",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "typeorm:generate": "npx typeorm-ts-node-esm migration:generate -d src/config/connection.ts",
    "typeorm:migrate": "npx typeorm-ts-node-esm migration:run -d src/config/connection.ts",
    "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d src/config/connection.ts",
    "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d src/config/connection.ts"
  },
  "keywords": [
    "TS",
    "ORM"
  ],
  "author": "Hussein Salah <Salahhussein176@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.4",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  }
}
